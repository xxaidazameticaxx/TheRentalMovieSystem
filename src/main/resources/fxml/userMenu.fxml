<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<GridPane alignment="CENTER" prefHeight="320.0" prefWidth="681.0" style="-fx-background-color: #212121;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.controllers.UserMenuController">
    <stylesheets>
        <URL value="@/css/userMenu.css" />
    </stylesheets>
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextField fx:id="welcomeTextField_id" accessibleRole="TEXT" alignment="CENTER" editable="false" focusTraversable="false" prefHeight="51.0" prefWidth="630.0" style="-fx-background-color: #212121;">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <GridPane alignment="CENTER" GridPane.rowIndex="1">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="315.0">
               <children>
                  <Button fx:id="allMoviesButton_id" alignment="CENTER" mnemonicParsing="false" onAction="#allMoviesClick" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: #c62828;" text="ALL MOVIES" textFill="WHITESMOKE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <Button fx:id="myMoviesButton_id" alignment="CENTER" mnemonicParsing="false" onAction="#myMoviesClick" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: #c62828;" text="MY MOVIES" textFill="WHITESMOKE">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </GridPane>
   </children>
</GridPane>
